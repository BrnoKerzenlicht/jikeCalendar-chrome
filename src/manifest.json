{
  "name": "jike-calendar",
  "description": "jike-calendar",
  "version": "1.0.3",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon/logo.png",
    "default_popup": "jike-calendar.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens jike-calendar.html"
    }
  }
}